{
  "name": "storefront-unit-tests",
  "version": "1.0.0",
  "description": "Unit tests for StoreFront with mocks and stubs",
  "main": "storefront.js",
  "scripts": {
    "test": "jest tests/",
    "test:watch": "jest tests/ --watch",
    "test:coverage": "jest tests/ --coverage",
    "test:mock": "jest tests/ --testNamePattern=\"mock\"",
    "test:stub": "jest tests/ --testNamePattern=\"stub\"",
    "test:basic": "jest tests/ --testNamePattern=\"basic\""
  },
  "devDependencies": {
    "jest": "^29.0.0"
  },
  "keywords": ["testing", "unit-tests", "mocks", "stubs", "jest"],
  "author": "Placide Nizero",
  "license": "MIT"
}
